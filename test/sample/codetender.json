{
  "tokens": [
    {
      "pattern": "CodeTender",
      "replacement": "Served"
    },
    {
      "pattern": "foo",
      "replacement": "bar"
    },
    {
      "pattern": "README.codetender.md",
      "replacement": "README.md"
    },
    {
      "pattern": "Won't find this anywhere",
      "replacement": "foo"
    }
  ],
  "ignore": [
    "README.md"
  ],
  "noReplace": [
    "noReplace-folder/",
    "no-replace-file.txt",
    "codetender.json",
    "foo/"
  ],
  "delete": [
    "delete-folder/",
    "delete-file.txt"
  ]
}